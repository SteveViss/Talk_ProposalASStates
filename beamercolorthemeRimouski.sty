\definecolor{foreground}{RGB}{105,104,101}
\definecolor{text}{RGB}{25,24,21}
\definecolor{set1}{RGB}{170,227,55}
\definecolor{set2}{RGB}{40,131,58}
\definecolor{set3}{RGB}{190,244,136}


\setbeamercolor{normal text}{fg=text}
\setbeamercolor{frametitle}{fg=set2}
\setbeamercolor{framesubtitle}{fg=text}

\setbeamercolor{title}{fg=set1}
\setbeamercolor{subtitle}{fg=black}

\setbeamercolor{alerted text}{fg=set2}
\setbeamercolor{structure}{fg=black}

\setbeamercolor{footlinecolor}{fg=set2}
\setbeamercolor{section in head/foot}{fg=black}

%%%%%%%%%%% Block color sets %%%%%%%%%%%%%%%%

% Standard block
\setbeamercolor{block title}{fg = white, bg = set2}
\setbeamercolor{block body}{bg = set3}

% Alert block
\setbeamercolor{block title alerted}{fg = uqarred, bg = uqarred!30}
\setbeamercolor{block body alerted}{ bg = uqarred!30}

% Example block
\setbeamercolor{block title example}{fg = uqargreen, bg = uqargreen!30}
\setbeamercolor{block body example}{ bg = uqargreen!30}

